{"ast":null,"code":"import { Plus } from '@element-plus/icons-vue';\nimport { productAPI } from '../api';\nexport default {\n  name: 'UserProducts',\n  components: {\n    Plus\n  },\n  data() {\n    return {\n      products: [],\n      loading: false,\n      searchQuery: '',\n      productStatus: '',\n      productCategory: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      confirmDialogVisible: false,\n      confirmDialogTitle: '',\n      confirmDialogContent: '',\n      actionProduct: null,\n      actionType: ''\n    };\n  },\n  computed: {\n    filteredProducts() {\n      let filtered = this.products;\n\n      // 搜索过滤\n      if (this.searchQuery) {\n        filtered = filtered.filter(product => product.name.toLowerCase().includes(this.searchQuery.toLowerCase()));\n      }\n\n      // 状态过滤\n      if (this.productStatus !== '') {\n        filtered = filtered.filter(product => product.status.toString() === this.productStatus);\n      }\n\n      // 分类过滤\n      if (this.productCategory !== '') {\n        filtered = filtered.filter(product => product.categoryId.toString() === this.productCategory);\n      }\n\n      // 分页\n      this.total = filtered.length;\n      const startIndex = (this.currentPage - 1) * this.pageSize;\n      const endIndex = startIndex + this.pageSize;\n      return filtered.slice(startIndex, endIndex);\n    },\n    emptyText() {\n      if (this.loading) return '加载中...';\n      return '暂无商品';\n    }\n  },\n  mounted() {\n    this.loadUserProducts();\n  },\n  watch: {\n    // 监听筛选条件变化，重置页码\n    searchQuery() {\n      this.currentPage = 1;\n    },\n    productStatus() {\n      this.currentPage = 1;\n    },\n    productCategory() {\n      this.currentPage = 1;\n    }\n  },\n  methods: {\n    async loadUserProducts() {\n      this.loading = true;\n      try {\n        const response = await productAPI.getUserProducts();\n        // 格式化数据\n        this.products = response.map(product => ({\n          ...product,\n          categoryName: this.getCategoryName(product.categoryId),\n          createdTime: this.formatDate(product.createdTime),\n          updateTime: this.formatDate(product.updateTime)\n        }));\n      } catch (error) {\n        this.$message.error('加载商品列表失败');\n        this.products = [];\n      } finally {\n        this.loading = false;\n      }\n    },\n    getCategoryName(categoryId) {\n      const categoryMap = {\n        1: '电子产品',\n        2: '图书文具',\n        3: '服装鞋帽',\n        4: '生活用品',\n        5: '其他'\n      };\n      return categoryMap[categoryId] || '未知';\n    },\n    getFirstImage(images) {\n      if (!images) return 'https://img.alicdn.com/imgextra/i4/O1CN01q0UcHt1h1sVt0jA8O_!!6000000004122-2-tps-112-112.png';\n      const imageArray = typeof images === 'string' ? JSON.parse(images) : images;\n      return imageArray && imageArray.length > 0 ? imageArray[0] : 'https://img.alicdn.com/imgextra/i4/O1CN01q0UcHt1h1sVt0jA8O_!!6000000004122-2-tps-112-112.png';\n    },\n    formatDate(dateString) {\n      if (!dateString) return '';\n      const date = new Date(dateString);\n      return date.toLocaleString('zh-CN');\n    },\n    goToCreate() {\n      this.$router.push('/product/create');\n    },\n    viewProduct(productId) {\n      this.$router.push(`/product/detail/${productId}`);\n    },\n    editProduct(productId) {\n      this.$router.push(`/product/update/${productId}`);\n    },\n    toggleStatus(product) {\n      this.actionProduct = product;\n      this.actionType = 'toggle';\n      this.confirmDialogTitle = product.status === 1 ? '确认下架商品' : '确认上架商品';\n      this.confirmDialogContent = product.status === 1 ? '下架后，商品将不再在首页显示，但可以随时重新上架。确定要下架该商品吗？' : '上架后，商品将在首页显示。确定要上架该商品吗？';\n      this.confirmDialogVisible = true;\n    },\n    deleteProduct(product) {\n      this.actionProduct = product;\n      this.actionType = 'delete';\n      this.confirmDialogTitle = '确认删除商品';\n      this.confirmDialogContent = '删除后将无法恢复，确定要删除该商品吗？';\n      this.confirmDialogVisible = true;\n    },\n    async confirmAction() {\n      this.confirmDialogVisible = false;\n      if (this.actionType === 'toggle') {\n        await this.performToggleStatus();\n      } else if (this.actionType === 'delete') {\n        await this.performDelete();\n      }\n    },\n    async performToggleStatus() {\n      const newStatus = this.actionProduct.status === 1 ? 0 : 1;\n      try {\n        await productAPI.updateProductStatus(this.actionProduct.id, newStatus);\n        // 更新本地数据\n        const index = this.products.findIndex(p => p.id === this.actionProduct.id);\n        if (index !== -1) {\n          this.products[index].status = newStatus;\n        }\n        this.$message.success(newStatus === 1 ? '商品上架成功' : '商品下架成功');\n      } catch (error) {\n        this.$message.error('操作失败，请重试');\n      }\n    },\n    async performDelete() {\n      try {\n        await productAPI.deleteProduct(this.actionProduct.id);\n        // 从本地数据中移除\n        this.products = this.products.filter(p => p.id !== this.actionProduct.id);\n        this.$message.success('商品删除成功');\n      } catch (error) {\n        this.$message.error('删除失败，请重试');\n      }\n    },\n    handleSizeChange(size) {\n      this.pageSize = size;\n    },\n    handleCurrentChange(current) {\n      this.currentPage = current;\n    }\n  }\n};","map":{"version":3,"names":["Plus","productAPI","name","components","data","products","loading","searchQuery","productStatus","productCategory","currentPage","pageSize","total","confirmDialogVisible","confirmDialogTitle","confirmDialogContent","actionProduct","actionType","computed","filteredProducts","filtered","filter","product","toLowerCase","includes","status","toString","categoryId","length","startIndex","endIndex","slice","emptyText","mounted","loadUserProducts","watch","methods","response","getUserProducts","map","categoryName","getCategoryName","createdTime","formatDate","updateTime","error","$message","categoryMap","getFirstImage","images","imageArray","JSON","parse","dateString","date","Date","toLocaleString","goToCreate","$router","push","viewProduct","productId","editProduct","toggleStatus","deleteProduct","confirmAction","performToggleStatus","performDelete","newStatus","updateProductStatus","id","index","findIndex","p","success","handleSizeChange","size","handleCurrentChange","current"],"sources":["D:\\文档\\学习\\专业课实验报告书\\专业选修\\软件工程\\version1\\frontend\\src\\views\\UserProducts.vue"],"sourcesContent":["<template>\n  <div class=\"user-products-container\">\n    <el-card class=\"products-card\">\n      <template #header>\n        <div class=\"card-header\">\n          <span>我的商品</span>\n          <el-button type=\"primary\" @click=\"goToCreate\" class=\"create-btn\">\n            <el-icon><Plus /></el-icon>\n            发布新商品\n          </el-button>\n        </div>\n      </template>\n      \n      <div class=\"search-filter\">\n        <el-input\n          v-model=\"searchQuery\"\n          placeholder=\"搜索商品名称\"\n          prefix-icon=\"el-icon-search\"\n          class=\"search-input\"\n          clearable\n        />\n        <el-select v-model=\"productStatus\" placeholder=\"选择状态\" clearable class=\"status-select\">\n          <el-option label=\"全部\" value=\"\" />\n          <el-option label=\"上架\" value=\"1\" />\n          <el-option label=\"下架\" value=\"0\" />\n        </el-select>\n        <el-select v-model=\"productCategory\" placeholder=\"选择分类\" clearable class=\"category-select\">\n          <el-option label=\"全部\" value=\"\" />\n          <el-option label=\"电子产品\" value=\"1\" />\n          <el-option label=\"图书文具\" value=\"2\" />\n          <el-option label=\"服装鞋帽\" value=\"3\" />\n          <el-option label=\"生活用品\" value=\"4\" />\n          <el-option label=\"其他\" value=\"5\" />\n        </el-select>\n      </div>\n      \n      <el-table\n        v-loading=\"loading\"\n        :data=\"filteredProducts\"\n        style=\"width: 100%\"\n        :empty-text=\"emptyText\"\n      >\n        <el-table-column prop=\"id\" label=\"商品ID\" width=\"80\" />\n        <el-table-column prop=\"name\" label=\"商品名称\" min-width=\"200\">\n          <template #default=\"scope\">\n            <div class=\"product-name-wrapper\">\n              <img :src=\"getFirstImage(scope.row.images)\" class=\"product-thumbnail\" />\n              <span class=\"product-name\">{{ scope.row.name }}</span>\n            </div>\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"categoryName\" label=\"分类\" width=\"100\" />\n        <el-table-column prop=\"price\" label=\"价格\" width=\"100\">\n          <template #default=\"scope\">¥{{ scope.row.price }}</template>\n        </el-table-column>\n        <el-table-column prop=\"status\" label=\"状态\" width=\"80\">\n          <template #default=\"scope\">\n            <el-tag :type=\"scope.row.status === 1 ? 'success' : 'info'\">\n              {{ scope.row.status === 1 ? '上架' : '下架' }}\n            </el-tag>\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"viewCount\" label=\"浏览量\" width=\"80\" />\n        <el-table-column prop=\"createdTime\" label=\"创建时间\" width=\"180\" />\n        <el-table-column prop=\"updateTime\" label=\"更新时间\" width=\"180\" />\n        <el-table-column label=\"操作\" width=\"200\" fixed=\"right\">\n          <template #default=\"scope\">\n            <el-button type=\"primary\" size=\"small\" @click=\"viewProduct(scope.row.id)\" class=\"btn-view\">\n              查看\n            </el-button>\n            <el-button type=\"success\" size=\"small\" @click=\"toggleStatus(scope.row)\" v-if=\"scope.row.status === 1\" class=\"btn-status\">\n              下架\n            </el-button>\n            <el-button type=\"success\" size=\"small\" @click=\"toggleStatus(scope.row)\" v-else class=\"btn-status\">\n              上架\n            </el-button>\n            <el-button type=\"warning\" size=\"small\" @click=\"editProduct(scope.row.id)\" class=\"btn-edit\">\n              编辑\n            </el-button>\n            <el-button type=\"danger\" size=\"small\" @click=\"deleteProduct(scope.row)\" class=\"btn-delete\">\n              删除\n            </el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n      \n      <div class=\"pagination-container\">\n        <el-pagination\n          v-model:current-page=\"currentPage\"\n          v-model:page-size=\"pageSize\"\n          :page-sizes=\"[10, 20, 50, 100]\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\"\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n        />\n      </div>\n    </el-card>\n    \n    <!-- 确认对话框 -->\n    <el-dialog v-model=\"confirmDialogVisible\" :title=\"confirmDialogTitle\" width=\"30%\">\n      <div>{{ confirmDialogContent }}</div>\n      <template #footer>\n        <span class=\"dialog-footer\">\n          <el-button @click=\"confirmDialogVisible = false\">取消</el-button>\n          <el-button type=\"danger\" @click=\"confirmAction\">确认</el-button>\n        </span>\n      </template>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport { Plus } from '@element-plus/icons-vue'\nimport { productAPI } from '../api'\n\nexport default {\n  name: 'UserProducts',\n  components: {\n    Plus\n  },\n  data() {\n    return {\n      products: [],\n      loading: false,\n      searchQuery: '',\n      productStatus: '',\n      productCategory: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      confirmDialogVisible: false,\n      confirmDialogTitle: '',\n      confirmDialogContent: '',\n      actionProduct: null,\n      actionType: ''\n    }\n  },\n  computed: {\n    filteredProducts() {\n      let filtered = this.products\n      \n      // 搜索过滤\n      if (this.searchQuery) {\n        filtered = filtered.filter(product => \n          product.name.toLowerCase().includes(this.searchQuery.toLowerCase())\n        )\n      }\n      \n      // 状态过滤\n      if (this.productStatus !== '') {\n        filtered = filtered.filter(product => product.status.toString() === this.productStatus)\n      }\n      \n      // 分类过滤\n      if (this.productCategory !== '') {\n        filtered = filtered.filter(product => product.categoryId.toString() === this.productCategory)\n      }\n      \n      // 分页\n      this.total = filtered.length\n      const startIndex = (this.currentPage - 1) * this.pageSize\n      const endIndex = startIndex + this.pageSize\n      return filtered.slice(startIndex, endIndex)\n    },\n    emptyText() {\n      if (this.loading) return '加载中...'\n      return '暂无商品'\n    }\n  },\n  mounted() {\n    this.loadUserProducts()\n  },\n  watch: {\n    // 监听筛选条件变化，重置页码\n    searchQuery() {\n      this.currentPage = 1\n    },\n    productStatus() {\n      this.currentPage = 1\n    },\n    productCategory() {\n      this.currentPage = 1\n    }\n  },\n  methods: {\n    async loadUserProducts() {\n      this.loading = true\n      try {\n        const response = await productAPI.getUserProducts()\n        // 格式化数据\n        this.products = response.map(product => ({\n          ...product,\n          categoryName: this.getCategoryName(product.categoryId),\n          createdTime: this.formatDate(product.createdTime),\n          updateTime: this.formatDate(product.updateTime)\n        }))\n      } catch (error) {\n        this.$message.error('加载商品列表失败')\n        this.products = []\n      } finally {\n        this.loading = false\n      }\n    },\n    getCategoryName(categoryId) {\n      const categoryMap = {\n        1: '电子产品',\n        2: '图书文具',\n        3: '服装鞋帽',\n        4: '生活用品',\n        5: '其他'\n      }\n      return categoryMap[categoryId] || '未知'\n    },\n    getFirstImage(images) {\n      if (!images) return 'https://img.alicdn.com/imgextra/i4/O1CN01q0UcHt1h1sVt0jA8O_!!6000000004122-2-tps-112-112.png'\n      const imageArray = typeof images === 'string' ? JSON.parse(images) : images\n      return imageArray && imageArray.length > 0 ? imageArray[0] : 'https://img.alicdn.com/imgextra/i4/O1CN01q0UcHt1h1sVt0jA8O_!!6000000004122-2-tps-112-112.png'\n    },\n    formatDate(dateString) {\n      if (!dateString) return ''\n      const date = new Date(dateString)\n      return date.toLocaleString('zh-CN')\n    },\n    goToCreate() {\n      this.$router.push('/product/create')\n    },\n    viewProduct(productId) {\n      this.$router.push(`/product/detail/${productId}`)\n    },\n    editProduct(productId) {\n      this.$router.push(`/product/update/${productId}`)\n    },\n    toggleStatus(product) {\n      this.actionProduct = product\n      this.actionType = 'toggle'\n      this.confirmDialogTitle = product.status === 1 ? '确认下架商品' : '确认上架商品'\n      this.confirmDialogContent = product.status === 1 ? '下架后，商品将不再在首页显示，但可以随时重新上架。确定要下架该商品吗？' : '上架后，商品将在首页显示。确定要上架该商品吗？'\n      this.confirmDialogVisible = true\n    },\n    deleteProduct(product) {\n      this.actionProduct = product\n      this.actionType = 'delete'\n      this.confirmDialogTitle = '确认删除商品'\n      this.confirmDialogContent = '删除后将无法恢复，确定要删除该商品吗？'\n      this.confirmDialogVisible = true\n    },\n    async confirmAction() {\n      this.confirmDialogVisible = false\n      \n      if (this.actionType === 'toggle') {\n        await this.performToggleStatus()\n      } else if (this.actionType === 'delete') {\n        await this.performDelete()\n      }\n    },\n    async performToggleStatus() {\n      const newStatus = this.actionProduct.status === 1 ? 0 : 1\n      try {\n        await productAPI.updateProductStatus(this.actionProduct.id, newStatus)\n        // 更新本地数据\n        const index = this.products.findIndex(p => p.id === this.actionProduct.id)\n        if (index !== -1) {\n          this.products[index].status = newStatus\n        }\n        this.$message.success(newStatus === 1 ? '商品上架成功' : '商品下架成功')\n      } catch (error) {\n        this.$message.error('操作失败，请重试')\n      }\n    },\n    async performDelete() {\n      try {\n        await 